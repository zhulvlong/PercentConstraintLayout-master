<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--为了方便扩展其他layout，定义在外层，命名以layout_开头，否则lint会报红警告-->
    <attr name="percent_constraint_radius" format="dimension|reference" /><!--圆角大小-->
    <attr name="percent_constraint_radiusLeftTop" format="dimension|reference" /><!--左上圆角大小-->
    <attr name="percent_constraint_radiusRightTop" format="dimension|reference" /><!--右上圆角大小-->
    <attr name="percent_constraint_radiusRightBottom" format="dimension|reference" /><!--左下圆角大小-->
    <attr name="percent_constraint_radiusLeftBottom" format="dimension|reference" /><!--右下圆角大小-->

    <attr name="percent_constraint_shadowColor" format="color|reference" /><!--阴影颜色-->
    <attr name="percent_constraint_shadowEvaluation" format="dimension|reference" /><!--阴影大小-->
    <attr name="percent_constraint_shadowDx" format="dimension|reference" /><!--阴影偏移X-->
    <attr name="percent_constraint_shadowDy" format="dimension|reference" /><!--阴影偏移Y-->

    <!--下面的这些百分比属性必须设置左右约束/上下约束才会生效-->
    <attr name="percent_constraint_width" format="string" />
    <attr name="percent_constraint_height" format="string" />
    <attr name="percent_constraint_marginLeft" format="string" /><!--控件先对容器(ConstraintLayout)左边距和容器宽度的百分比-->
    <attr name="percent_constraint_marginRight" format="string" /><!--控件先对容器(ConstraintLayout)右边距和容器宽度的百分比-->
    <attr name="percent_constraint_marginStart" format="string" /><!--控件先对容器(ConstraintLayout)左边距和容器宽度的百分比-->
    <attr name="percent_constraint_marginEnd" format="string" /><!--控件先对容器(ConstraintLayout)右边距和容器宽度的百分比-->
    <attr name="percent_constraint_marginTop" format="string" /><!--控件先对容器(ConstraintLayout)上边距和容器高度的百分比-->
    <attr name="percent_constraint_marginBottom" format="string" /><!--控件先对容器(ConstraintLayout)下边距和容器高度的百分比-->

    <!--用统一一个PercentLayout，用于封装读取自定义属性-->
    <declare-styleable name="PercentLayout">
        <attr name="percent_constraint_radius" />
        <attr name="percent_constraint_radiusLeftTop" />
        <attr name="percent_constraint_radiusRightTop" />
        <attr name="percent_constraint_radiusRightBottom" />
        <attr name="percent_constraint_radiusLeftBottom" />
        <attr name="percent_constraint_shadowColor" />
        <attr name="percent_constraint_shadowEvaluation" />
        <attr name="percent_constraint_shadowDx" />
        <attr name="percent_constraint_shadowDy" />
        <attr name="percent_constraint_width" />
        <attr name="percent_constraint_height" />
        <attr name="percent_constraint_marginLeft" />
        <attr name="percent_constraint_marginRight" />
        <attr name="percent_constraint_marginTop" />
        <attr name="percent_constraint_marginBottom" />
        <attr name="percent_constraint_marginStart" />
        <attr name="percent_constraint_marginEnd" />
    </declare-styleable>
    <declare-styleable name="PercentLinearLayout_Layout">
        <attr name="percent_constraint_radius" />
        <attr name="percent_constraint_radiusLeftTop" />
        <attr name="percent_constraint_radiusRightTop" />
