<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.app">

    <!-- 访问网络，网络定位需要上网-->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PercentConstraintLayoutActivity" />
        <activity android:name=".SubEditTextActivity" />
        <activity android:name=".SubTextViewActivity" />
        <activity android:name=".SubImageViewActivity" />
        <activity android:name=".SubViewGroupActivity" />
        <activity android:name=".SubViewActivity" />
        <activity android:name=".GradientActivity" />
        <activity android:name=".CheckedActivity" />
        <activity android:name=".RipperActivity" />
        <activity android:name=".ShadowActivity" />
    </application>

</manifest>